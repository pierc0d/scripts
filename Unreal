debounce = true
game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Died:Connect(function()
	wait(3)
	print('gave ability')
	game.ReplicatedStorage.ToggleAbility:InvokeServer(true)
end)
game.ReplicatedStorage.TakeQuest:FireServer("Rigged Game")
game.ReplicatedStorage.ToggleAbility:InvokeServer(true)
repeat
    if workspace.John then
        local John = workspace.John.Humanoid
        repeat
            game.ReplicatedStorage.Damage:FireServer("Detonate",John,18,{})
            wait(0.0001)
        until John.Health <= 0
    else
       print("Waiting for a John")
    end
    if workspace.Arlo then
        print("ArloTime")
       local Arlo = workspace.Arlo.Humanoid
        repeat
            for i, v in pairs(game.Workspace:GetChildren()) do
                if v:IsA("Model") and v.Name == "Barrier" then
                    game.ReplicatedStorage.Cast:FireServer("Detonate",{
                        ["Mousepos"] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
                        ["Charge"] = 1
                    },0)
                    game.ReplicatedStorage.Damage:FireServer("Detonate",v.Humanoid,18,{})
                end
            end
            game.ReplicatedStorage.Damage:FireServer("Detonate",Arlo,18,{})
            wait(0.0001)
        until Arlo.Health <= 0
    end
    wait(0.001)
until debounce == false
