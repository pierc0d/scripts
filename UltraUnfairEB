function KillAll()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "John" or "Seraphina" or "Arlo" then
            if v:FindFirstChild("Humanoid") then
game.ReplicatedStorage.Cast:FireServer(("Create Blade"),{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position,["Charge"] = 0},0)
game.ReplicatedStorage.Damage:FireServer("Throw",v:FindFirstChild("Humanoid"),0,{[1] = v:FindFirstChild("HumanoidRootPart"),["BladesLeft"] = 1, ["Indirect"] = true})
            end
        end
    end
end
function AutoQuest()
    wait(10)
    game.ReplicatedStorage.TakeQuest:FireServer("Rigged Game")
    wait(10)
    game.ReplicatedStorage.TakeQuest:FireServer("Completed")
end
while true do
    KillAll()
    wait(0.001)
end
