MiniLVL = 6
SlotNum = 2
function Spin()
    --// Settings \\--
    getgenv().Slot = SlotNum -- Slot To Store Ability
    getgenv().MinLevel = MiniLVL -- Minimum Level To Keep An Ability
    getgenv().Whitelisted_Abilities = {
        ""
    } -- Abilities to keep regardless of potential
    getgenv().AutoSpin = not getgenv().AutoSpin -- This will enable and disable the script on each execution, no need to edit just re-execute to disable
    if not getgenv().RanScript then
        --// Backup Settings \\--
        if not getgenv().Slot and not getgenv().MinLevel and not getgenv().Whitelisted_Abilities then
            getgenv().Slot = 2 -- Slot To Store Ability
            getgenv().MinLevel = 2 -- Minimum Level To Keep An Ability
            getgenv().Whitelisted_Abilities = {
            } -- Abilities to keep regardless of potential
        end
    
        getgenv().Normal_Delay = 0 -- Delay For Abilities Less Than 5.0 
        getgenv().High_Delay = 0.1 -- Delay For Abilities Higher Than 5.0 this does nothing atm lolll
    
        --// DON'T EDIT ANYTHING UNDER HERE UNLESS YOU KNOW WHAT YOU'RE DOING LOL \\--
    
        local s = game:GetService("Players").LocalPlayer.PlayerGui.Reroll.Reroll
        if not getgenv().Ability_Function then
            getgenv().Ability_Function = nil
        end
        
        local old;
    
        for i,v in pairs(getgc(true)) do
            if type(v) == "function" and getfenv(v).script == s and getinfo(v).name == "OnClientInvoke" and tostring(getconstants(v)[1]) ~= "bosstp" then
                if not getgenv().Ability_Function then
                    getgenv().Ability_Function = v
                end
            end
        end
    
        getgenv().Reroll_Function = function(a,b)
            getgenv().Slot2 = nil
    
            print(a,b)
            task.wait(getgenv().Normal_Delay)
            if table.find(getgenv().Whitelisted_Abilities, a) then
                getgenv().woo = messagebox("Would You Like To Store "..a.."( "..b.." )?", "Got A Whitelisted Ability!", 4)
                if getgenv().woo == 6 then
                    getgenv().Slot2 = getgenv().Slot
                elseif getgenv().woo == 4 then
                    getgenv().Slot2 = false
                else
                    getgenv().Slot2 = false
                end
            end
            if b >= MinLevel then
                getgenv().woo = messagebox("Would You Like To Store "..a.."( "..b.." )?", "Ability Above Selected Potential", 4)
                if getgenv().woo == 6 then
                    getgenv().Slot2 = getgenv().Slot
                elseif getgenv().woo == 4 then
                    getgenv().Slot2 = false
                else
                    getgenv().Slot2 = false
                end
            end
            if getgenv().Slot2 == nil then
                getgenv().Slot2 = false
            end
    
            return false, getgenv().Slot2
        end
    
        old = hookfunction(getgenv().Ability_Function, function(a,b) 
            return getgenv().Reroll_Function(a, b)
        end)
    
        task.spawn(function()
            while true do
                if getgenv().AutoSpin then
                    task.spawn(function()
                        game.ReplicatedStorage.Reroll:InvokeServer()
                    end)
                end
                task.wait()
            end
        end)
        getgenv().RanScript = true
    end
end
function KillMobs()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Cripple" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Isen" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Blyke" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Arlo" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Barrier" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "John" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Seraphina" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Remi" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Zeke" then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if v:FindFirstChild("Humanoid") and dist <= 100 then
                game.ReplicatedStorage.Damage:FireServer("Dark Blade",v:FindFirstChild("Humanoid"))
            end
        end
    end
end
function IceSeraphina()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Cripple" and v:FindFirstChild("Humanoid") then
             game.ReplicatedStorage.Damage:FireServer("Icicle",v:FindFirstChild("Humanoid"),0,{["Type"] = "Ice",["Charge"] = 100})
        end
    end
end
function BomberSeraphina()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "Seraphina" and v:FindFirstChild("Humanoid") then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
    end
end
function BomberGodTierFarm()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v.Name == "John" and v:FindFirstChild("Humanoid") then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
        if v:IsA("Model") and v.Name == "Arlo" and v:FindFirstChild("Humanoid") then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
        if v:IsA("Model") and v.Name == "Barrier" and v:FindFirstChild("Humanoid") then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
        if v:IsA("Model") and v.Name == "Seraphina" and v:FindFirstChild("Humanoid") then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
    end
end
function BomberKAInf()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Name ~= game.Players.LocalPlayer.Character.Name then
            game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
            game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
        end
    end
end
function BomberKA()
    for i, v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Name ~= game.Players.LocalPlayer.Character.Name then
            local dist = (game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - v:FindFirstChild("HumanoidRootPart").Position).Magnitude
            if dist < 40 then
                game.ReplicatedStorage.Damage:FireServer("Detonate",v:FindFirstChild("Humanoid"),18,{})
                game.ReplicatedStorage.Cast:FireServer("Detonate",{["Mousepos"] = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Position"),["Charge"] = 1},0)
            end
        end
    end
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pierc0d's Hub : https://discord.gg/ty4z3354gc", "GrapeTheme")
if game.Players.LocalPlayer.UserId == 375420630 or 1729572302 then
    local AdminPanel = Window:NewTab("Admin Panel")
    local Admin = AdminPanel:NewSection("Admin")
    local AdminBAura = 0
    Admin:NewToggle("Admin Bomber Aura", "", function(state)
    debounce = false
    if state then
        debounce = true
        while debounce == true do
            if AdminBAura == 1 then
                BomberGodTierFarm()
            end
            if AdminBAura == 2 then
                BomberKAInf()
            end
            wait(0.01)
        end
    else
        debounce = false
    end
end)
    Admin:NewDropdown("Admin Bomber Settings", "DropdownInf", {"God Tier Farm", "Kill Aura Infinite Distance"}, function(currentOption)
    if currentOption == "God Tier Farm" then
        AdminBAura = 1
    end
    if currentOption == "Kill Aura Infinite Distance" then
        AdminBAura = 2
    end
end)
end
local UnFair = Window:NewTab("UnFair")
local FunctionsSection = UnFair:NewSection("Functions")
FunctionsSection:NewToggle("Kill Aura (Arbiter)", "Kills anyone around you.", function(state)
    debounce = false 
    if state then
        debounce = true
        while debounce == true do
            KillMobs()
            wait(0.05)
        end
    else
        debounce = false
    end
end)
local UnReal = Window:NewTab("UnReal")
local UnRealSection = UnReal:NewSection("Kill Aura")
local BAura = 0
UnRealSection:NewToggle("Bomber Aura", "", function(state)
    debounce = false
    if state then
        debounce = true
        while debounce == true do
            if BAura == 1 then
                BomberSeraphina()
            end
            if BAura == 2 then
                BomberKA()
            end
            wait(0.01)
        end
    else
        debounce = false
    end
end)
UnRealSection:NewDropdown("Bomber Settings", "DropdownInf", {"Seraphina Farm", "Kill Aura"}, function(currentOption)
    if currentOption == "Seraphina Farm" then
        BAura = 1
    end
    if currentOption == "Kill Aura" then
        BAura = 2
    end
end)
local EAura = 0
local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Spinning")
MiscSection:NewToggle("Auto Quest 4", "Completes and gives you the Quest when ready.", function(state)
    debounce = false 
    if state then
        debounce = true
        game.ReplicatedStorage.TakeQuest:FireServer("Rigged Game")
        while debounce == true do
            game.ReplicatedStorage.TakeQuest:FireServer("Completed")
            if game.Players.LocalPlayer.PlayerGui.MainClient.Quest.Folder.Objective.progress.Text == "15/15" then
                wait(.5)
                game.ReplicatedStorage.TakeQuest:FireServer("Rigged Game")
            end
            wait(1)
        end
    else
        debounce = false
    end
end)
local Spinning = Misc:NewSection("Spinning (Randomly Stores)")
Spinning:NewToggle("Spin Ability (Press again to toggle off)", "Spins your Ability pretty quickly", function(state)
    debounce = false 
    if state then
        Spin()
    else
        Spin()
    end
end)
Spinning:NewDropdown("Ability Store", "What ability it will store into", {"1", "2", "3"}, function(currentOption)
    if currentOption == "1" then
        SlotNum = 1
    end
    if currentOption == "2" then
        SlotNum = 2
    end
    if currentOption == "3" then
        SlotNum = 3
    end
end)
Spinning:NewDropdown("Ability Minimum Level", "", {"3.5","5","6"}, function(currentOption)
    if currentOption == "3.5" then
        MiniLVL = 3.5
    end
    if currentOption == "5" then
        MiniLVL = 5
    end
    if currentOption == "6" then
        MiniLVL = 6
    end
end)
local Credits = Window:NewTab("Information")
local VersionSection = Credits:NewSection("Version: 1.0")
local CreditsSection = Credits:NewSection("Credits")
CreditsSection:NewTextBox("MADE BY: PIERC0D", "TextboxInfo", function(txt)
	print(txt)
end)
CreditsSection:NewKeybind("P to Toggle", "", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)
